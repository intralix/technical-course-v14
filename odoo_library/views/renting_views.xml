<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    <record id="renting_view_form" model="ir.ui.view">
        <field name="name">library.renting.form</field>
        <field name="model">library.renting</field>
        <field name="arch" type="xml">        
        <form string="Renting Form">
            <sheet>
               <h1><field name="name" /></h1>
               <group>              
                  <field name="nameclient" />
                  <field name="startdate" />     
                  <field name="enddate" />     
                  <field name="phone" />                                                                    
               </group>               
            </sheet>
        </form>    
        </field>
    </record>            
    </data>
    
<record id="renting_view_list" model="ir.ui.view">
     <field name="name">library.renting.list</field>
     <field name="model">library.renting</field>
     <field name="arch" type="xml">    
          <tree string="Renting List">
          <field name="name"/>
          <field name="client_id"/>
          </tree>
       </field>
</record>
</odoo>

<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.actions.act_window" id="space_list_action">
            <field name="name"> Spaceships </field>
            <field name="res_model">space.space</field>
            <field name="view_mode">tree.form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create the first spaceship
                </p>
            </field>
        </record>
        
        <menuitem name="Odoo Spaceships"
                  id="menu_space"
                  sequence="1"
                  />
        
        <menuitem id="menu_space_sub" name="Odoo Spaceships" parent="menu_space" sequence="2"/>
        
        <menuitem id="menu_space_ships" name="Spaceships" parent="menu_space_sub" sequence="3"
                  action="space_list_action"/>
        
        
    </data>  
</odoo>
<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="group_volunteer_admin" model="res.groups" >
        <field name="name" >Volunteers / Principal</field>
    </record>
    <record id="group_volunteer_volunteer" model="res.groups" >
        <field name="name" >Volunteers / Volunteer</field>
    </record>
    <record id="only_active_modify" model="ir.rule" >
        <field name="name" >Only Active Course Can Be Modified</field>
        <field name="perm_read" eval="0" ></field>
        <field name="perm_write" eval="1" ></field>
        <field name="perm_create" eval="0" ></field>
        <field name="perm_unlike" eval="1" ></field>
        <field name="domain_force" >[('active','=,'True)]</field>
        <field name="model_id" >model_cooperative_volunteers</field>
    </record>
</odoo>